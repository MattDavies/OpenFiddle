@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-type" content="text/html; charset=utf-8" />
    <title>OpenFiddle</title>
    <link rel="icon" type="image/vnd.microsoft.icon" href="~/Content/images/favicon.ico" />
    @Styles.Render("~/sitecss")
    @Scripts.Render("~/sitescripts")
    @if (ViewBag.Editor == true)
    {
        @Scripts.Render("~/editor")
        <meta name="Description" content="Coding tool that allows you to save and send executable snippets of code to others.">
    }
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-221584-37', 'openfiddle.com');
        ga('send', 'pageview');
    </script>
</head>
<body>
    @RenderBody()
    <div class="ui-layout-south">A side project by <a href="http://twitter.com/mdaviesnet">@@mdaviesnet</a></div>
</body>
</html>